#include<stdio.h>
void main()
{
    int i,j;
    char ch="A';
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=5;j++)
        {
            if(i==1||1==5||j==1||j==5)
            {
                printf("*");
            }
            else
            {
                printf("%c",ch++);
            }
        }
        printf("\n")
    }
}
